@model IEnumerable<MassagerApp.PL.ViewModels.ChatViewModel>
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<div class="list-group">
    @foreach (var item in Model)
    {
        <div class="my-10">
            <a href=@("/massage/" + item.Id) class="list-group-item list-group-item-action">@item.UserId</a>
        </div>
    }
</div>